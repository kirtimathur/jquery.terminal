import java.util.*;

public class Main {

  public static void main(String[] args) {
    // Write your code here
    Scanner scn= new Scanner(System.in);
    int n= scn.nextInt();
    int r= scn.nextInt();
    
    int nfact=1;
    for(int i=n;i>=1;i--){
      nfact=nfact*i;
    }
    int nmrfact=1;
    for(int i=n-r;i>=1,i--){
      nmrfact=nmrfact*i;
    }
    int npr=nfact/nmrfact;
    System.out.print(npr);
  }
}
